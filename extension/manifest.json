{
    "manifest_version": 3,
    "name": "Hello Extensions of the world",
    "description": "Base Level Extension",
    "version": "1.0",
    "action": {
        "default_popup": "hello.html",
        "default_icon": "favicon-16x16.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": [
        "*://*/*"
    ],
    "permissions": [
        "tabs",
        "activeTab"
    ]
}
